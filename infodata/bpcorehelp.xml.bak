	<FUNCTION NAME="LEFT" DESCRIPTION="Returns the leftmost 'n' characters from StrFrom." HELPRESOURCEID="LRM.HTML#LEFT">
		<PARAMETER NAME="StrFrom" TYPE="String" ISOPTIONAL="FALSE" DESCRIPTION="String from which to extract characters."/>
		<PARAMETER NAME="Length" TYPE="Number" ISOPTIONAL="FALSE" DESCRIPTION="Numbers of Characters to Extract."/>
	</FUNCTION>
	<FUNCTION NAME="RIGHT" DESCRIPTION="Returns the Rightmost 'n' characters from StrFrom." HELPRESOURCEID="LRM.HTML#RIGHT">
		<PARAMETER NAME="StrFrom" TYPE="String" ISOPTIONAL="FALSE" DESCRIPTION="String from which to extract characters."/>
		<PARAMETER NAME="Length" TYPE="Number" ISOPTIONAL="FALSE" DESCRIPTION="Numbers of Characters to Extract."/>
	</FUNCTION>
	<FUNCTION NAME="MID" DESCRIPTION="Extracts 'length' characters from 'strfrom' at 'pos'. if length is not specified, the rest of the string will be returned." HELPRESOURCEID="LRM.HTML#MID">
	<PARAMETER NAME="String" TYPE="String" ISOPTIONAL="FALSE" DESCRIPTION="String from which to extract characters."/>
	<PARAMETER NAME="Start" TYPE="Number" ISOPTIONAL="FALSE" DESCRIPTION="Location within the string at which to extract characters."/>
	<PARAMETER NAME="Length" TYPE="Number" ISOPTIONAL="TRUE" DESCRIPTION="Length of the string to extract from the first argument. If this would extend past the end of the string or is missing, the rest of the string is returned."/>
	
	
	
	
	</FUNCTION>
