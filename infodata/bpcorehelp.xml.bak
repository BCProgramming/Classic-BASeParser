	 <FUNCTION NAME="SEQ" DESCRIPTION="Creates a array containing a sequence of values using an expression, variable to change within the aforementioned expression, an initial value, a end value, and an optional Step." HELPRESOURCEID="SEQ.HTML">
	 	<PARAMETER NAME="Expression" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Expression used to create the sequence each iteration."/>
	 	<PARAMETER NAME="Varname" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Variable name to cycle from Start to end. Generally, this is found within the Expression argument."/>
	 	<PARAMETER NAME="Start" TYPE="Number" ISOPTIONAL="FALSE" DESCRIPTION="Starting value for 'Varname' argument."/>
	 	<PARAMETER NAME="End" TYPE="Number" ISOPTIONAL="FALSE" DESCRIPTION="Ending value for 'Varname' argument."/>
	 	<PARAMETER NAME="Step" TYPE="Number" ISOPTIONAL="TRUE" DESCRIPTION="number used to increment 'Varname' after each iteration. if not provided, defaults to"/>
	 	
	
	 </FUNCTION>
	<FUNCTION NAME="SEQEX" DESCRIPTION="Creates a List of values with the given parameters, which include a initial value, and expression to perform, the name of the variable to increment, the expression used to change the variable, and an expression that,  when true, will terminate calculations." HELPRESOURCEID="SEQEX.HTML">
	<PARAMETER NAME="Expression" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Expression that operates on the given variable name for each iteration."/>
	<PARAMETER NAME="VarName" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Name of the variable to use."/>
	<PARAMETER NAME="InitialValue" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Initial value to assign to VarName"/>
	<PARAMETER NAME="IncrementExpression" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Expression executed each loop and whose result will be assigned to the given variable name."/>
	<PARAMETER NAME="TerminateExpression" TYPE="Direct" ISOPTIONAL="FALSE" DESCRIPTION="Expression executed each loop. If the return is non-zero (true) the loop is terminated and so are further calculations. The current result will be returned at that point."/>
	</FUNCTION>
	
