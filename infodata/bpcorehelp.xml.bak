	<FUNCTION NAME="DATEADD" DESCRIPTION="Adds a given Number of Intervals to Date.">
		<PARAMETER NAME="Interval" TYPE="String" DESCRIPTION="Interval to add to Date. (yyyy=Year,q=Quarter,m=Month,y=Day of Year,d=Day,w=weekday,WW=Week,h=Hour,n=Minute,s=Second)"/>
		<PARAMETER NAME="Amount" TYPE="Number" DESCRIPTION="Number of Intervals to add to Date."/>
		<PARAMETER NAME="Date" TYPE="Date" DESCRIPTION="Date to add intervals to."/>
	
	</FUNCTION>
	<FUNCTION NAME="DATEDIFF" DESCRIPTION="Returns the difference between two dates in the given unit of time.">
		<!--DateDiff(Interval,Date1,Date2,[FirstDayOfWeek])-->
		<PARAMETER NAME="Interval" TYPE="String" DESCRIPTION="Unit of time to return the difference in."/>
		<PARAMETER NAME="Date1" TYPE="Date" DESCRIPTION="older date to compare to Date2"/>
		<PARAMETER NAME="Date2" TYPE="Date" DESCRIPTION="More recent date then Date1"/>
		<PARAMETER NAME="FirstDayOfWeek" TYPE="DayOfWeekConstant" ISOPTIONAL="True" DESCRIPTION="First day of week to use in the calculation."/>

		
	</FUNCTION>
	<!--'DatePart(Interval,Date,FirstDayOfWeek)-->
	<FUNCTION NAME="DATEPART" DESCRIPTION="Returns a specified component of the given date.">
		<PARAMETER NAME="Interval" TYPE="String" DESCRIPTION="Portion of the date to retrieve."/>
		<PARAMETER NAME="Date" TYPE="Date" DESCRIPTION="Date to parse."/>
	
	
	
	</FUNCTION>
	<FUNCTION NAME="DATESERIAL" DESCRIPTION="Converts a Month,Day, and Year Parameter into a Date Variable.">
		<!--DateSerial(Year,Month,Day) -->
		<PARAMETER NAME="Year" TYPE="Number" DESCRIPTION="Year of the Date to create."/>
		<PARAMETER NAME="Month" TYPE="Number" DESCRIPTION="Month of the Date to create. If Month Exceeds 12, the returned date will be within the following year(s)"/>
		<PARAMETER NAME="Day" TYPE="Number" DESCRIPTION="Day of the Date to create. If Day exceeds the number of days in the month of the returned date, then the month is incremented. If this increment causes Month to Exceed 12, the date will go into the following year."/>
	
	
	</FUNCTION>
	
		
